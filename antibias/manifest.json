{
  "name": "Antibias",
  "version": "1.1",
  "description": "Remove Names and photos from linkedin profiles",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"], 
      "web_accessible_resources": ["img/pattern.jpg"],
      "css" : ["css/style.css"],
      "js" : ["js/script.js"]
    }
  ],
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "manifest_version": 2
}
